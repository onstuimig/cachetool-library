        "symfony/http-client": "^6.0",
        "symfony/http-foundation": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Onstuimig",
            "homepage": "https://onstuimig.nl"
        },
        {
            "name": "gordalina",
            "email": "samuel.gordalina@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
